{
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "Kreact.createElement",
        "pragmaFrag": "Kreact.fragment"
      }
    ]
  ],
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [">= 1%"]
        }
      }
    ]
  ]
}
